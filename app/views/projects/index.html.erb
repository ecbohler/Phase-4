<div>

  <form class="search box fl-r">
    <label for='Search It'>Search: </label>
    <input type="text_field" />
    <input type="submit" value="SEARCH" />
  </form>

<div class="c-hero-challenge"></div>

<h2 class="c-hero-container_pages">PROJECTS</h2>

<div class="c-hero-container-outer">
</div>

  <br>

  <div>
    <% @projects.each do |project| %>
    <div class="chal-box">

      <img src="data:image/png;base64,"></img><br>

      <div class="user-name">
        <p><%= link_to "#{project.title}", user_project_path(id: project.id), class: "projects-index" %></p>
      </div><br>

      <div class="user-name">
        <p>Title: <%= project.title %></p>
      </div><br>

      <div class="user-name">
        <p>Description: <%= project.description[0..50] + '...' %></p>
      </div><br>

      <div class="user-name">
        <p>created by: <%= User.find(project.user_id).name %></p><br>
      </div>
      <br>
    </div>
    <% end %>
  </div>
</div>
