 <section class="c-hero-user-profile">

  <h2 class="c-hero-container_pages">User Profile</h2>
  <div class="c-hero-container-outer"></div>

</section>

<div class="asset-render">
  <span style="display: none" class="asset-class-type"><%= @user.class %></span>
  <span style="display: none" class="asset-class-id"><%= @user.id %></span>
  <span style="display: none" class="user-id"><%= session[:user_id] %></span>
<div class="page-container-all">
  <div class="user-container-all">
    <div class="user-pic">
      <img src="<%= @user.gravatar_url %>" width="250" height="250"/>
    </div>
      <div class="user-content-box">
         <form>
          <input type="submit" value="Edit Profile" class="user-edit-button">
        </form>
      <span class="user-name-title"><%= @user.name %></span>
      <hr class="user-content" />
      <span class="user-cohort-title">Fiery Skippers - 2015</span><br /><br />
      <span class="user-content-titles">Email:</span>
      <span class="user-content-body"><%= link_to @user.email %></span>
        <br /><br />
      <span class="user-content-titles">Github:</span>
      <span class="user-content-body"><%= link_to @user.github %></span>
      <br /><br />
      <span class="user-content-titles">Location:</span>
      <span class="user-content-body"><%= @user.location %></span>
      <br /><br />
      <span class="user-content-titles">Facebook:</span>
      <span class="user-content-body"><%= link_to @user.facebook %></span>
      <br /><br />
      <span class="user-content-titles">Twitter_handle:</span>
      <span class="user-content-body"><%= @user.twitter_handle %></span>
      <br /><br />
      <span class="user-content-titles">Linked-In:</span>
      <span class="user-content-body"><%= link_to @user.linkedin %></span>
        <br />
    </div>
    <!-- PROJECTS START -->
    <div class="user-owned-content">
      <% if @projects.length != 0 %>
        <div class="user-name-title">Projects</div>
        <% @projects.each do |p| %>
              <div class="chal-box-user">

      <img src="data:image/png;base64," width="155px"></img><br>

      <div class="user-owned-title">
        <p><%= link_to "#{p.title}", user_project_path(user_id: p.id) %></p>
      </div><br>

      <div class="user-owned-title">
        <p>Title: <%= p.title %></p>
      </div><br>

      <div class="user-owned-title">
        <p>Description: <%= p.description[0..50] + '...' %></p>
      </div><br>
      <br>
    </div>
        <% end %>
      <% end %>
<!-- PROJECTS END -->
<!-- CHALLENGES START -->
    <div class="user-owned-content">
      <% if @challenges.length != 0 %>
        <div class="user-name-title">Challenges</div>
        <% @challenges.each do |p| %>
          <div class="chal-box-user">
          <img src="data:image/png;base64," width="155px"></img><br>

          <div class="user-owned-title">
          <p><%= link_to "#{p.title}", user_challenge_path(user_id: p.id) %></p>
          </div><br>

          <div class="user-owned-title">
            <p>Title: <%= p.title %></p>
          </div><br>

          <div class="user-owned-title">
            <p>Description: <%= p.description[0..50] + '...' %></p>
          </div>
          <br /><br />
      </div>
      <% end %>
    <% end %>
<!-- CHALLENGES END -->

    </div>
    <%= link_to "Back to Members", users_path %>
    <br />
  <!-- USER CONTAINER END -->
  </div>
</div>
  <br /><br />
</div>



