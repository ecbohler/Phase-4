 <h1>Challenge Show Page</h1><br>

  <br/ ><br/ >

<div class="asset-render">

  <span style="display:none" class="user-id"><%= current_user.id %></span>
  <span style="display:none" class="asset-class-type"><%=@challenge.class  %></span>
  <span style="display:none" class="asset-class-id"><%= @challenge.id %></span>

  <strong>Title:</strong> <%= @challenge.title %><br>
  Description: <%= @challenge.description %><br>
  Github url: <%= @challenge.github_url %><br>
  External url: <%= @challenge.external_url %><br>
  Image url: <%= @challenge.image_url %><br>
  Difficulty: <%= @challenge.difficulty %><br>


  <br /><br />

  <!-- COMMENTS/SOLUTIONS LINKS START -->
    <a href="#" class="show-comments">View Comments</a> |
    <a href="#">View Solution</a>
  <!-- COMMENTS/SOLUTIONS LINKS END -->

  <br /><br />

  <!-- TAG FORM START -->
    <form class="asset-tag-form">
      <label for='Tag It'>Tag this: </label>
      <input type="text_field" />
      <input type="submit" value="Tag It" />
    </form>
  <!-- TAG FORM END -->
<P> This is the current users</p>
  <!-- VOTES START -->
    <span>Votes: <%= @challenge.votes.count %></span> -
    <a href ="/votes" class="asset-create-vote">
    <%= image_tag('thumbs_up.jpg', width:'18') %></a>
  <!-- VOTES END -->

  <br /><br />

  <%= link_to "Challenges Home", user_challenges_path(@challenge.user_id) %>

</div>

