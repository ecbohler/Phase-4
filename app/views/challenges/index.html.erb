<div>
<div class="tag-search-form">
  <form class="search box fl-r" action="/users/<%= current_user.id %>/challenges" method="get">
    <label for='Search It'>Search: </label>
    <input type="text" name="search_string"/>
    <input type="submit" value="SEARCH" />
  </form>
</div>


<div class="c-hero-challenge"></div>

<h2 class="c-hero-container_pages">CHALLENGES</h2>

<div class="c-hero-container-outer">
</div>

<br>

  <div>
    <% @challenges.each do |challenge| %>
    <div class="chal-box">

      <img src="data:image/jpeg;base64,"></img><br>

      <div class="user-name">
        <p><%= link_to "#{challenge.title}", user_challenge_path(id: challenge.id),class: "projects-index" %></p>
      </div><br>

      <div class="user-name">
        <p>Title: <%= challenge.title %></p>
      </div><br>

           <div class="user-name">
        <p>created by: <%= User.find(challenge.user_id).name %></p><br>
      </div>

    </div>
    <% end %>
  </div>
</div>
