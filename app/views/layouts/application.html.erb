<!DOCTYPE html>
<html>
<head>
  <title>Phase4test</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

  <header class="p-sm cf ">
    <h1 class="fl-l ">
      <img src="http://devbootcamp.com/assets/img/devbootcamp-logo.png"></img>
    </h1>
    <nav class="fl-r ">
      <ul class="cf  phase4-nav">

        <li><%= link_to 'HOME', root_path %></li>
        <li><%= link_to 'MEMBERS', users_path %></li>

        <li><%= link_to 'CHALLENGES', user_challenges_path(session[:user_id]) %></li>

        <li><%= link_to 'RESOURCES', user_resources_path(session[:user_id]) %></li>

        <li><%= link_to 'PROJECTS', user_projects_path(session[:user_id]) %></li>

        <li>
          <div class="login">
            <%if current_user%>
            Welcome <%=current_user.name%>
            <% else %>
            LOGIN
            <br>
            WITH GITHUB
            <% end %>
          </div>
        </li>
      </ul>
    </nav>
  </header>
  <%= yield %>

</body>
</html>
